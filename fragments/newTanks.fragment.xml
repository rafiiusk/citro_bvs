<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
	<Dialog contentWidth="100%" verticalScrolling="true">
		<content>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="6" labelSpanS="6"
				adjustLabelSpan="false" emptySpanXL="2" emptySpanL="2" emptySpanM="2" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2">
				<f:content>
					<l:VerticalLayout id="msgNewTanks" visible="false" class="sapUiContentPadding" width="100%"/>
					<Label text="{i18n>CodEmbrc}" required="true" labelFor="inpTankCodEmbrc"/>
					<Input id="inpTankCodEmbrc" editable="false" type="Text" maxLength="40"/>
					<Label text="{i18n>NomeEmbrc}" required="true" labelFor="inpTankNomeEmbrc"/>
					<Input id="inpTankNomeEmbrc" editable="false" type="Text" maxLength="40"/>
					<Label text="{i18n>Hatch}" required="true" labelFor="inpTankHatch"/>
					<Input id="inpTankHatch" type="Text" maxLength="2"/>
					<Label text="{i18n>Tanque}" required="true" labelFor="inpTankTanque"/>
					<Input id="inpTankTanque" type="Text" maxLength="60"/>
					<Label text="{i18n>Capacidade}" required="false" labelFor="inpTankCapacidade"/>
					<Input id="inpTankCapacidade" type="Number" maxLength="10"/>
				</f:content>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="{i18n>cancel}" press="onCancelNewTanks"></Button>
		</beginButton>
		<endButton>
			<Button text="{i18n>save}" press="onConfirmNewTank"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>